{
    "name": "ipublishingjp/itc-reporter",
    "description": "PHP package for downloading stats from iTunes Connect - Replacement for Apples Java based Reporter.jar",
    "keywords": ["iTunes", "Connect", "Reporter.jar", "App Stats"],
    "homepage": "https://github.com/mikebarlow/itc-reporter",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Barlow",
            "email": "mike@mikebarlow.co.uk",
            "role": "Developer"
        },
        {
            "name": "Yukimune Takagi",
            "email": "takagi@ipublishing.jp",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "name": "snscripts/result",
            "type": "vcs",
            "url": "https://github.com/ipublishingjp/result"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "6.*",
        "snscripts/result": "dev-master",
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "squizlabs/php_codesniffer": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Snscripts\\ITCReporter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Snscripts\\ITCReporter\\Tests\\": "tests"
        }
    }
}
